<h1>Latest Report</h1>

<h3>Users</h3>
<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Name</th>
      <th>Username</th>
    </tr>
  </thead>
  <tbody>
    {{#each report.users}}
    <tr>
      <td>{{id}}</td>
      <td>{{first_name}} {{last_name}}</td>
      <td>{{username}}</td>
    </tr>
    {{/each}}
  </tbody>
</table>

<h3>Customers</h3>
<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Company</th>
      <th>Account Manager ID</th>
    </tr>
  </thead>
  <tbody>
    {{#each report.customers}}
    <tr>
      <td>{{id}}</td>
      <td>{{company_name}}</td>
      <td>{{account_manager_id}}</td>
    </tr>
    {{/each}}
  </tbody>
</table>

<h3>Orders</h3>
<table>
  <thead>
    <tr>
      <th>ID</h>
      <th>Fulfilled</th>
      <th>Content ID</th>
      <th>Customer ID</th>
    </tr>
  </thead>
  <tbody>
    {{#each report.orders}}
    <tr>
      <td>{{id}}</td>
      <td>{{isFulfilled}}</td>
      <td>{{content}}</td>
      <td>{{customer_id}}</td>
    </tr>
    {{/each}}
  </tbody>
</table>

<h3>Products</h3>
<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Name</th>
      <th>Price</th>
      <th>Stock</th>
    </tr>
  </thead>
  <tbody>
    {{#each report.products}}
    <tr>
      <td>{{id}}</td>
      <td>{{item_name}}</td>
      <td>{{price}}</td>
      <td>{{stock}}</td>
    </tr>
    {{/each}}
  </tbody>
</table>